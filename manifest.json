{
  "background":{
    "scripts": ["popup.js"]
  },

  "manifest_version": 2,

  "name": "Intent",
  "description": "This extension will help you be more mindful of your browsing",
  "version": "1.0",
  
  "browser_action": {
   "default_icon": "icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab"
   ]
}